<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Parallel failure_threshold="-1" success_threshold="1">
            <Sequence>
                <Action ID="SubtaskControlZx120Arm" goal_position="-40" initial_position="0"/>
                <Action ID="SubtaskControlZx120Boom" goal_position="90" initial_position="0"/>
            </Sequence>
            <Sequence>
                <Action ID="SubtaskControlZx120Swing" goal_position="70" initial_position="80"/>
                <Action ID="SubtaskControlZx120Bucket" goal_position="20" initial_position="30"/>
            </Sequence>
        </Parallel>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="SubtaskControlZx120Arm">
            <input_port name="goal_position"/>
            <input_port name="initial_position"/>
        </Action>
        <Action ID="SubtaskControlZx120Boom">
            <input_port name="goal_position"/>
            <input_port name="initial_position"/>
        </Action>
        <Action ID="SubtaskControlZx120Bucket">
            <input_port name="goal_position"/>
            <input_port name="initial_position"/>
        </Action>
        <Action ID="SubtaskControlZx120Swing">
            <input_port name="goal_position"/>
            <input_port name="initial_position"/>
        </Action>
    </TreeNodesModel>
    <!-- ////////// -->
</root>
