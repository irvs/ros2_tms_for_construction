                            <Action ID="BlackboardValueWriterMongo" input_value="true" mongo_param_name="ARRIVAL_FLG" mongo_record_name="SAMPLE_BOARD_1"/>
                        </Sequence>
                        <Delay delay_msec="5000">
                            <AlwaysSuccess/>
                        </Delay>
                        <AlwaysFailure/>
                    </Switch2>
                </Parallel>
            </KeepRunningUntilFailure>
        </Sequence>
    </BehaviorTree>
