<?xml version="1.0"?>
<root main_tree_to_execute="MainTree">
    <!-- ////////// -->
    <BehaviorTree ID="MainTree">
        <Sequence name="root_sequence">
            <SetBlackboard output_key="sample_param_local" value="0"/>
            <SetBlackboard output_key="sample_param_global" value="0"/>
            <Action  model_name="zx120" ID="LeafNodeSampleZx120" record_name="boom_joint" subtask_name="st_zx120_sample_boom"/>
            <Action ID="BlackboardValueChecker" key="sample_param_local" value="{sample_param_local}"/>
            <Action ID="BlackboardValueChecker" key="sample_param_global" value="{sample_param_global}"/>
            <Action  model_name="zx120" ID="LeafNodeSampleZx120" record_name="swing_joint" subtask_name="st_zx120_sample_swing"/>
            <Action ID="BlackboardValueChecker" key="sample_param_global" value="{sample_param_global}"/>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="BlackboardValueChecker">
            <inout_port name="key"/>
            <inout_port name="value"/>
        </Action>
        <Action ID="BlackboardValueWriterSrv">
            <inout_port name="output_key"/>
            <inout_port name="value"/>
        </Action>
        <Action ID="BlackboardValueWriterTopic">
            <inout_port name="output_key"/>
            <inout_port name="value"/>
        </Action>
        <Action ID="LeafNodeIc120">
            <input_port default="ic120" name=" model_name"/>
            <inout_port name="record_name"/>
            <inout_port name="subtask_name"/>
        </Action>
        <Action ID="LeafNodeSampleZx120">
            <input_port default="zx120" name=" model_name"/>
            <inout_port name="record_name"/>
            <inout_port name="subtask_name"/>
        </Action>
        <Action ID="LeafNodeSampleZx200">
            <input_port default="zx200" name=" model_name"/>
            <inout_port name="record_name"/>
            <inout_port name="subtask_name"/>
        </Action>
        <Action ID="LeafNodeZx200">
            <input_port default="zx200" name=" model_name"/>
            <inout_port name="record_name"/>
            <inout_port name="subtask_name"/>
        </Action>
    </TreeNodesModel>
    <!-- ////////// -->
</root>
