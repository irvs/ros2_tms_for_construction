<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Sequence>
            <Action ID="MongoValueWriter" input_value="false" mongo_param_name="INITIAL_POS_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508"/>
            <Action ID="MongoValueWriter" input_value="false" mongo_param_name="SENSING_NEXTPOINT_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508"/>
            <Action ID="MongoValueWriter" input_value="false" mongo_param_name="ARRIVAL_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508"/>
            <Action ID="MongoValueWriter" input_value="false" mongo_param_name="SENSING_ARRIVAL_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508"/>
            <Action ID="MongoValueWriter" input_value="false" mongo_param_name="LOADED_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508"/>
            <Action ID="MongoValueWriter" input_value="false" mongo_param_name="SENSING_LOADED_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508"/>
            <Decorator ID="KeepRunningUntilFlgup" key="TERMINATE_FLG">
                <Sequence>
                    <Action ID="BlackboardValueReaderMongo" mongo_param_name="CONTINUE_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508" output_port="CONTINUE_FLG"/>
                    <WhileDoElse>
                        <Action ID="ConditionalExpression" conditional_expression="CONTINUE_FLG == true"/>
                        <Sequence>
                            <Action ID="BlackboardValueReaderMongo" mongo_param_name="INITIAL_POS_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508" output_port="INITIAL_POS_FLG"/>
                            <IfThenElse>
                                <Action ID="ConditionalExpression" conditional_expression="INITIAL_POS_FLG == false"/>
                                <Sequence>
                                    <Action ID="WaitForClick" button_info="initial_pose_待機" window_title="initial_pose_待機"/>
                                    <Action ID="LeafNodeZx200" model_name="zx200" name="leaf_node_zx200" record_name="initial_pose" subtask_name="subtask_zx200_change_pose"/>
                                </Sequence>
                                <AlwaysSuccess/>
                            </IfThenElse>
                            <Action ID="MongoValueWriter" input_value="true" mongo_param_name="INITIAL_POS_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508"/>
                            <Parallel failure_threshold="1" success_threshold="-1">
                                <Action ID="BlackboardValueReaderMongo" mongo_param_name="INITIAL_POS_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508" output_port="INITIAL_POS_FLG"/>
                                <Action ID="BlackboardValueReaderMongo" mongo_param_name="ARRIVAL_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508" output_port="ARRIVAL_FLG"/>
                                <Action ID="BlackboardValueReaderMongo" mongo_param_name="SENSING_ARRIVAL_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508" output_port="SENSING_ARRIVAL_FLG"/>
                                <Action ID="BlackboardValueReaderMongo" mongo_param_name="LOADED_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508" output_port="LOADED_FLG"/>
                                <Action ID="BlackboardValueReaderMongo" mongo_param_name="SENSING_LOADED_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508" output_port="SENSING_LOADED_FLG"/>
                                <Action ID="BlackboardValueReaderMongo" mongo_param_name="SENSING_CHECK_MOUND_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508" output_port="SENSING_CHECK_MOUND_FLG"/>
                                <Action ID="BlackboardValueReaderMongo" mongo_param_name="SENSING_NEXTPOINT_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508" output_port="SENSING_NEXTPOINT_FLG"/>
                                <IfThenElse>
                                    <Action ID="ConditionalExpression" conditional_expression="ARRIVAL_FLG == true and SENSING_ARRIVAL_FLG == true and LOADED_FLG == false and SENSING_LOADED_FLG == false and SENSING_CHECK_MOUND_FLG == false "/>
                                    <Sequence>
                                        <Action ID="MongoValueWriter" input_value="false" mongo_param_name="INITIAL_POS_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508"/>
                                        <RetryUntilSuccessful num_attempts="-1">
                                            <Fallback>
                                                <Action ID="LeafNodeZx200" model_name="zx200" name="leaf_node_zx200" record_name="target_excavate_pose" subtask_name="subtask_zx200_excavate_simple_plan"/>
                                                <Delay delay_msec="5000">
                                                    <AlwaysFailure/>
                                                </Delay>
                                            </Fallback>
                                        </RetryUntilSuccessful>
                                        <Sequence>
                                            <Action ID="WaitForClick" button_info="target_excavate_pose_待機" window_title="target_excavate_pose_待機"/>
                                            <Action ID="LeafNodeZx200" model_name="zx200" name="leaf_node_zx200" record_name="target_excavate_pose" subtask_name="subtask_zx200_excavate_simple"/>
                                        </Sequence>
                                        <Sequence>
                                            <Action ID="WaitForClick" button_info="hold_soil_while_elevating_pose_待機" window_title="hold_soil_while_elevating_pose_待機"/>
                                            <Action ID="LeafNodeZx200" model_name="zx200" name="leaf_node_zx200" record_name="hold_soil_while_elevating_pose" subtask_name="subtask_zx200_change_pose"/>
                                        </Sequence>
                                        <Sequence>
                                            <Action ID="WaitForClick" button_info="hold_soil_while_turning_pose_待機" window_title="hold_soil_while_turning_pose_待機"/>
                                            <Action ID="LeafNodeZx200" model_name="zx200" name="leaf_node_zx200" record_name="hold_soil_while_turning_pose" subtask_name="subtask_zx200_change_pose"/>
                                        </Sequence>
                                        <RetryUntilSuccessful num_attempts="-1">
                                            <Fallback>
                                                <Action ID="LeafNodeZx200" model_name="zx200" name="leaf_node_zx200" record_name="target_release_pose" subtask_name="subtask_zx200_change_pose_plan"/>
                                                <Delay delay_msec="5000">
                                                    <AlwaysFailure/>
                                                </Delay>
                                            </Fallback>
                                        </RetryUntilSuccessful>
                                        <Sequence>
                                            <Action ID="WaitForClick" button_info="release_pose_待機" window_title="release_pose_待機"/>
                                            <Action ID="LeafNodeZx200" model_name="zx200" name="leaf_node_zx200" record_name="target_release_pose" subtask_name="subtask_zx200_change_pose"/>
                                        </Sequence>
                                        <Sequence>
                                            <Action ID="WaitForClick" button_info="release_angle_待機" window_title="release_angle_待機"/>
                                            <Action ID="LeafNodeZx200" model_name="zx200" name="leaf_node_zx200" record_name="release_angle" subtask_name="subtask_zx200_release_simple"/>
                                        </Sequence>
                                        <Sequence>
                                            <Action ID="WaitForClick" button_info="after_release_soil_pose_待機" window_title="after_release_soil_pose_待機"/>
                                            <Action ID="LeafNodeZx200" model_name="zx200" name="leaf_node_zx200" record_name="after_release_soil_pose" subtask_name="subtask_zx200_change_pose"/>
                                        </Sequence>
                                        <Delay delay_msec="3000">
                                            <AlwaysSuccess/>
                                        </Delay>
                                    </Sequence>
                                    <IfThenElse>
                                        <Action ID="ConditionalExpression" conditional_expression="( SENSING_LOADED_FLG == true or SENSING_CHECK_MOUND_FLG == true  ) and ARRIVAL_FLG == true and SENSING_ARRIVAL_FLG == true  and INITIAL_POS_FLG == true"/>
                                        <Sequence>
                                            <WhileDoElse>
                                                <Action ID="ConditionalExpression" conditional_expression="ARRIVAL_FLG == true and SENSING_ARRIVAL_FLG == true"/>
                                                <Sequence>
                                                    <Action ID="MongoValueWriter" input_value="true" mongo_param_name="MOVING_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508"/>
                                                    <Action ID="BlackboardValueReaderMongo" mongo_param_name="ARRIVAL_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508" output_port="ARRIVAL_FLG"/>
                                                    <Action ID="BlackboardValueReaderMongo" mongo_param_name="SENSING_ARRIVAL_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508" output_port="SENSING_ARRIVAL_FLG"/>
                                                </Sequence>
                                                <Action ID="MongoValueWriter" input_value="false" mongo_param_name="MOVING_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508"/>
                                            </WhileDoElse>
                                            <IfThenElse>
                                                <Action ID="ConditionalExpression" conditional_expression="SENSING_CHECK_MOUND_FLG == true"/>
                                                <Action ID="MongoValueWriter" input_value="false" mongo_param_name="CONTINUE_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508"/>
                                                <AlwaysSuccess/>
                                            </IfThenElse>
                                        </Sequence>
                                        <Action ID="MongoValueWriter" input_value="false" mongo_param_name="MOVING_FLG" mongo_record_name="SAMPLE_BLACKBOARD_SIPDEMO202508"/>
                                    </IfThenElse>
                                </IfThenElse>
                            </Parallel>
                        </Sequence>
                        <SetBlackboard output_key="TERMINATE_FLG" value="true"/>
                    </WhileDoElse>
                </Sequence>
            </Decorator>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="BlackboardValueReaderMongo">
            <input_port name="mongo_param_name" type="std::string"/>
            <input_port name="mongo_record_name" type="std::string"/>
            <input_port name="output_port" type="std::string"/>
        </Action>
        <Action ID="ConditionalExpression">
            <input_port name="conditional_expression" type="std::string"/>
        </Action>
        <Action ID="Counter">
            <input_port name="key"/>
        </Action>
        <Decorator ID="KeepRunningUntilFlgup">
            <input_port name="key"/>
        </Decorator>
        <Action ID="LeafNodeD37pxi">
            <input_port default="d37pxi" name=" model_name"/>
            <inout_port name="record_name"/>
            <inout_port name="subtask_name"/>
        </Action>
        <Action ID="LeafNodeIc120">
            <input_port default="ic120" name=" model_name"/>
            <inout_port name="record_name"/>
            <inout_port name="subtask_name"/>
        </Action>
        <Action ID="LeafNodeMst110cr">
            <input_port default="mst110cr" name=" model_name"/>
            <inout_port name="record_name"/>
            <inout_port name="subtask_name"/>
        </Action>
        <Action ID="LeafNodeMst2200">
            <input_port default="mst2200" name=" model_name"/>
            <inout_port name="record_name"/>
            <inout_port name="subtask_name"/>
        </Action>
        <Action ID="LeafNodeZx200">
            <input_port name="model_name" type="float"/>
            <input_port name="record_name" type="float"/>
            <input_port name="subtask_name" type="float"/>
        </Action>
        <Action ID="MongoValueWriter">
            <input_port name="input_value" type="std::string"/>
            <input_port name="mongo_param_name" type="std::string"/>
            <input_port name="mongo_record_name" type="std::string"/>
        </Action>
        <Action ID="SetLocalBlackboard">
            <inout_port name="output_key"/>
            <input_port name="value"/>
        </Action>
        <Action ID="SetLocalBlackboardWithCounter">
            <inout_port name="counter_key"/>
            <input_port name="key"/>
        </Action>
        <Action ID="WaitForClick">
            <input_port name="button_info"/>
            <input_port name="window_title"/>
        </Action>
    </TreeNodesModel>
    <!-- ////////// -->
</root>
