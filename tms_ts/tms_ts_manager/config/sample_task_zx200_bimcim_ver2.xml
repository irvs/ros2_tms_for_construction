<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Sequence>
            <SetBlackboard output_key="N" value="1"/>
            <Action ID="MongoValueWriter" input_value="false" mongo_param_name="TURN_FINISHED_FLG" mongo_record_name="global_blackboard"/>
            <Action ID="MongoValueWriter" input_value="false" mongo_param_name="INITIAL_POS_FLG" mongo_record_name="global_blackboard"/>
            <Action ID="MongoValueWriter" input_value="false" mongo_param_name="ARRIVAL_FLG" mongo_record_name="global_blackboard"/>
            <Action ID="MongoValueWriter" input_value="false" mongo_param_name="LOADED_FLG" mongo_record_name="global_blackboard"/>
            <Action ID="MongoValueWriter" input_value="false" mongo_param_name="DUMP_LAUNCHED_FLG" mongo_record_name="global_blackboard"/>
            <Action ID="MongoValueWriter" input_value="false" mongo_param_name="FINISH_FLG" mongo_record_name="global_blackboard"/>
            <Action ID="BlackboardValueReaderMongo" mongo_param_name="num" mongo_record_name="total_num_of_dig_location" output_port="num"/>
            <Action ID="SetLocalBlackboard" output_key="number_of_dig_cycle_n" value="number_of_dig_cycle_1"/>
            <Action ID="SetLocalBlackboard" output_key="initial_position_n" value="initial_position_1"/>
            <Action ID="SetLocalBlackboard" output_key="initial_pose_n" value="initial_pose_1"/>
            <Action ID="SetLocalBlackboard" output_key="excavate_pose_n" value="excavate_pose_1"/>
            <Action ID="SetLocalBlackboard" output_key="hold_soil_while_elevating_pose_n" value="hold_soil_while_elevating_pose_1"/>
            <Action ID="SetLocalBlackboard" output_key="hold_soil_while_turning_pose_n" value="hold_soil_while_turning_pose_1"/>
            <Action ID="SetLocalBlackboard" output_key="release_pose_n" value="release_pose_1"/>
            <Action ID="SetLocalBlackboard" output_key="release_angle_n" value="release_angle_1"/>
            <Action ID="SetLocalBlackboard" output_key="after_release_soil_pose_n" value="after_release_soil_pose_1"/>
            <Action ID="SetLocalBlackboard" output_key="counter_flg_1" value="false"/>
            <Action ID="SetLocalBlackboard" output_key="counter_flg_2" value="false"/>
            <SetBlackboard output_key="PointN" value="1"/>
            <Action ID="SetLocalBlackboard" output_key="N_FLG" value="false"/>
            <Decorator ID="KeepRunningUntilFlgup" key="counter_flg_1">
                <Sequence>
                    <SetBlackboard output_key="dig_num" value="0"/>
                    <Action ID="SetLocalBlackboardWithCounter" counter_key="N" key="number_of_dig_cycle_n"/>
                    <Action ID="SetLocalBlackboardWithCounter" counter_key="N" key="initial_position_n"/>
                    <Action ID="SetLocalBlackboardWithCounter" counter_key="N" key="initial_pose_n"/>
                    <Action ID="BlackboardValueReaderMongo" mongo_param_name="dig_cycle" mongo_record_name="{number_of_dig_cycle_n}" output_port="dig_cycle"/>
                    <Action  model_name="zx200" ID="LeafNodeZx200" record_name="{initial_position_n}" subtask_name="st_zx200_follow_waypoints"/>
                    <Action  model_name="zx200" ID="LeafNodeZx200" record_name="{initial_pose_n}" subtask_name="subtask_zx200_change_pose"/>
                    <Action ID="MongoValueWriter" input_value="true" mongo_param_name="INITIAL_POS_FLG" mongo_record_name="global_blackboard"/>
                    <Action ID="MongoValueWriter" input_value="false" mongo_param_name="FINISH_FLG" mongo_record_name="global_blackboard"/>
                    <Action ID="SetLocalBlackboard" output_key="counter_flg_2" value="false"/>
                    <Action ID="MongoValueWriter" input_value="false" mongo_param_name="MOVING_FLG" mongo_record_name="global_blackboard"/>
                    <Decorator ID="KeepRunningUntilFlgup" key="counter_flg_2">
                        <Sequence>
                            <Action ID="BlackboardValueReaderMongo" mongo_param_name="TURN_FINISHED_FLG" mongo_record_name="global_blackboard" output_port="TURN_FINISHED_FLG"/>
                            <Action ID="BlackboardValueReaderMongo" mongo_param_name="INITIAL_POS_FLG" mongo_record_name="global_blackboard" output_port="INITIAL_POS_FLG"/>
                            <Action ID="BlackboardValueReaderMongo" mongo_param_name="ARRIVAL_FLG" mongo_record_name="global_blackboard" output_port="ARRIVAL_FLG"/>
                            <Action ID="BlackboardValueReaderMongo" mongo_param_name="LOADED_FLG" mongo_record_name="global_blackboard" output_port="LOADED_FLG"/>
                            <Action ID="BlackboardValueReaderMongo" mongo_param_name="DUMP_LAUNCHED_FLG" mongo_record_name="global_blackboard" output_port="DUMP_LAUNCHED_FLG"/>
                            <Action ID="BlackboardValueReaderMongo" mongo_param_name="FINISH_FLG" mongo_record_name="global_blackboard" output_port="FINISH_FLG"/>
                            <IfThenElse>
                                <Action ID="ConditionalExpression" conditional_expression="ARRIVAL_FLG == true and LOADED_FLG != true and INITIAL_POS_FLG == true and TURN_FINISHED_FLG != true"/>
                                <Sequence>
                                    <Action ID="SetLocalBlackboardWithCounter" counter_key="N" key="excavate_pose_n"/>
                                    <Action ID="SetLocalBlackboardWithCounter" counter_key="N" key="hold_soil_while_elevating_pose_n"/>
                                    <Action ID="SetLocalBlackboardWithCounter" counter_key="N" key="hold_soil_while_turning_pose_n"/>
                                    <Action ID="SetLocalBlackboardWithCounter" counter_key="N" key="release_pose_n"/>
                                    <Action ID="SetLocalBlackboardWithCounter" counter_key="N" key="release_angle_n"/>
                                    <Action ID="SetLocalBlackboardWithCounter" counter_key="N" key="after_release_soil_pose_n"/>
                                    <Action ID="MongoValueWriter" input_value="false" mongo_param_name="INITIAL_POS_FLG" mongo_record_name="global_blackboard"/>
                                    <Action  model_name="zx200" ID="LeafNodeZx200" record_name="{excavate_pose_n}" subtask_name="subtask_zx200_excavate_simple"/>
                                    <Action  model_name="zx200" ID="LeafNodeZx200" record_name="{hold_soil_while_elevating_pose_n}" subtask_name="subtask_zx200_change_pose"/>
                                    <Action  model_name="zx200" ID="LeafNodeZx200" record_name="{hold_soil_while_turning_pose_n}" subtask_name="subtask_zx200_change_pose"/>
                                    <Action  model_name="zx200" ID="LeafNodeZx200" record_name="{release_pose_n}" subtask_name="subtask_zx200_change_pose"/>
                                    <Action  model_name="zx200" ID="LeafNodeZx200" record_name="{release_angle_n}" subtask_name="subtask_zx200_release_simple"/>
                                    <Action  model_name="zx200" ID="LeafNodeZx200" record_name="{after_release_soil_pose_n}" subtask_name="subtask_zx200_change_pose"/>
                                    <Action  model_name="zx200" ID="LeafNodeZx200" record_name="{initial_pose_n}" subtask_name="subtask_zx200_change_pose"/>
                                    <Action ID="MongoValueWriter" input_value="true" mongo_param_name="INITIAL_POS_FLG" mongo_record_name="global_blackboard"/>
                                    <Action ID="Counter" key="dig_num"/>
                                    <IfThenElse>
                                        <Action ID="ConditionalExpression" conditional_expression="dig_num == dig_cycle"/>
                                        <Action ID="MongoValueWriter" input_value="true" mongo_param_name="TURN_FINISHED_FLG" mongo_record_name="global_blackboard"/>
                                        <AlwaysSuccess/>
                                    </IfThenElse>
                                </Sequence>
                                <IfThenElse>
                                    <Action ID="ConditionalExpression" conditional_expression="ARRIVAL_FLG == true and ( LOADED_FLG == true or TURN_FINISHED_FLG == true ) and INITIAL_POS_FLG == true and FINISH_FLG != true"/>
                                    <IfThenElse>
                                        <Action ID="ConditionalExpression" conditional_expression="TURN_FINISHED_FLG == true "/>
                                        <Sequence>
                                            <IfThenElse>
                                                <Action ID="ConditionalExpression" conditional_expression="N_FLG == false"/>
                                                <Sequence>
                                                    <Action ID="Counter" key="N"/>
                                                    <Action ID="MongoValueWriter" input_value="{N}" mongo_param_name="N" mongo_record_name="global_blackboard"/>
                                                    <Action ID="SetLocalBlackboard" output_key="N_FLG" value="true"/>
                                                </Sequence>
                                                <AlwaysSuccess/>
                                            </IfThenElse>
                                            <IfThenElse>
                                                <Action ID="ConditionalExpression" conditional_expression="PointN == num"/>
                                                <Sequence>
                                                    <Action ID="SetLocalBlackboard" output_key="counter_flg_1" value="true"/>
                                                    <Action ID="SetLocalBlackboard" output_key="counter_flg_2" value="true"/>
                                                    <Action ID="MongoValueWriter" input_value="true" mongo_param_name="FINISH_FLG" mongo_record_name="global_blackboard"/>
                                                </Sequence>
                                                <AlwaysSuccess/>
                                            </IfThenElse>
                                            <Action ID="BlackboardValueReaderMongo" mongo_param_name="DUMP_LAUNCHED_FLG" mongo_record_name="global_blackboard" output_port="DUMP_LAUNCHED_FLG"/>
                                            <IfThenElse>
                                                <Action ID="ConditionalExpression" conditional_expression="DUMP_LAUNCHED_FLG == false "/>
                                                <Action ID="MongoValueWriter" input_value="true" mongo_param_name="MOVING_FLG" mongo_record_name="global_blackboard"/>
                                                <Sequence>
                                                    <Action ID="BlackboardValueReaderMongo" mongo_param_name="ARRIVAL_NEXT_FLG" mongo_record_name="global_blackboard" output_port="ARRIVAL_NEXT_FLG"/>
                                                    <WhileDoElse>
                                                        <Action ID="ConditionalExpression" conditional_expression="ARRIVAL_NEXT_FLG == false"/>
                                                        <AlwaysSuccess/>
                                                        <Sequence>
                                                            <Action ID="SetLocalBlackboard" output_key="counter_flg_2" value="true"/>
                                                            <Action ID="MongoValueWriter" input_value="false" mongo_param_name="MOVING_FLG" mongo_record_name="global_blackboard"/>
                                                            <Action ID="MongoValueWriter" input_value="false" mongo_param_name="TURN_FINISHED_FLG" mongo_record_name="global_blackboard"/>
                                                            <Action ID="MongoValueWriter" input_value="false" mongo_param_name="DUMP_LAUNCHED_FLG" mongo_record_name="global_blackboard"/>
                                                            <Action ID="MongoValueWriter" input_value="false" mongo_param_name="ARRIVAL_NEXT_FLG" mongo_record_name="global_blackboard"/>
                                                            <Action ID="Counter" key="PointN"/>
                                                            <Action ID="SetLocalBlackboard" output_key="N_FLG " value="false"/>
                                                        </Sequence>
                                                    </WhileDoElse>
                                                </Sequence>
                                            </IfThenElse>
                                        </Sequence>
                                        <WhileDoElse>
                                            <Action ID="ConditionalExpression" conditional_expression="DUMP_LAUNCHED_FLG == false"/>
                                            <Action ID="MongoValueWriter" input_value="true" mongo_param_name="MOVING_FLG" mongo_record_name="global_blackboard"/>
                                            <Sequence>
                                                <Action ID="MongoValueWriter" input_value="false" mongo_param_name="MOVING_FLG" mongo_record_name="global_blackboard"/>
                                                <Action ID="MongoValueWriter" input_value="false" mongo_param_name="LOADED_FLG" mongo_record_name="global_blackboard"/>
                                                <Action ID="MongoValueWriter" input_value="false" mongo_param_name="DUMP_LAUNCHED_FLG" mongo_record_name="global_blackboard"/>
                                            </Sequence>
                                        </WhileDoElse>
                                    </IfThenElse>
                                    <IfThenElse>
                                        <Action ID="ConditionalExpression" conditional_expression="FINISH_FLG == true"/>
                                        <Sequence>
                                            <Action ID="SetLocalBlackboard" output_key="counter_flg_1" value="true"/>
                                            <Action ID="SetLocalBlackboard" output_key="counter_flg_2" value="true"/>
                                        </Sequence>
                                        <Action ID="MongoValueWriter" input_value="false" mongo_param_name="MOVING_FLG" mongo_record_name="global_blackboard"/>
                                    </IfThenElse>
                                </IfThenElse>
                            </IfThenElse>
                        </Sequence>
                    </Decorator>
                </Sequence>
            </Decorator>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="BlackboardValueReaderMongo">
            <input_port name="mongo_param_name"/>
            <input_port name="mongo_record_name"/>
            <input_port name="output_port"/>
        </Action>
        <Action ID="ConditionalExpression">
            <input_port name="conditional_expression"/>
        </Action>
        <Action ID="Counter">
            <input_port name="key"/>
        </Action>
        <Decorator ID="KeepRunningUntilFlgup">
            <input_port name="key"/>
        </Decorator>
        <Action ID="LeafNodeD37pxi">
            <input_port default="d37pxi" name=" model_name"/>
            <inout_port name="record_name"/>
            <inout_port name="subtask_name"/>
        </Action>
        <Action ID="LeafNodeIc120">
            <input_port default="ic120" name=" model_name"/>
            <inout_port name="record_name"/>
            <inout_port name="subtask_name"/>
        </Action>
        <Action ID="LeafNodeMst110cr">
            <input_port default="mst110cr" name=" model_name"/>
            <inout_port name="record_name"/>
            <inout_port name="subtask_name"/>
        </Action>
        <Action ID="LeafNodeMst2200">
            <input_port default="mst2200" name=" model_name"/>
            <inout_port name="record_name"/>
            <inout_port name="subtask_name"/>
        </Action>
        <Action ID="LeafNodeZx200">
            <input_port default="zx200" name=" model_name"/>
            <inout_port name="record_name"/>
            <inout_port name="subtask_name"/>
        </Action>
        <Action ID="MongoValueWriter">
            <input_port name="input_value"/>
            <input_port name="mongo_param_name"/>
            <input_port name="mongo_record_name"/>
        </Action>
        <Action ID="SetLocalBlackboard">
            <inout_port name="output_key"/>
            <input_port name="value"/>
        </Action>
        <Action ID="SetLocalBlackboardWithCounter">
            <inout_port name="counter_key"/>
            <input_port name="key"/>
        </Action>
    </TreeNodesModel>
    <!-- ////////// -->
</root>
