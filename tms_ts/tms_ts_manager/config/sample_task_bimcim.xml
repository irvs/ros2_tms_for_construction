<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Sequence>
            <SetBlackboard output_key="N" value="0"/>
            <Action ID="SetLocalBlackboard" output_key="NumX" value="Num0"/>
            <Repeat num_cycles="100">
                <Sequence>
                    <Action ID="Counter" key="N"/>
                    <Action ID="SetLocalBlackboardWithCounter" counter_key="N" key="NumX"/>
                    <Delay delay_msec="1000">
                        <AlwaysSuccess/>
                    </Delay>
                </Sequence>
            </Repeat>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="BlackboardValueReaderMongo">
            <input_port name="mongo_param_name"/>
            <input_port name="mongo_record_name"/>
            <input_port name="output_port"/>
        </Action>
        <Action ID="ConditionalExpression">
            <input_port name="conditional_expression"/>
        </Action>
        <Action ID="Counter">
            <input_port name="key"/>
        </Action>
        <Decorator ID="KeepRunningUntilFlgup">
            <input_port name="key"/>
        </Decorator>
        <Action ID="LeafNodeD37pxi">
            <input_port default="d37pxi" name=" model_name"/>
            <inout_port name="record_name"/>
            <inout_port name="subtask_name"/>
        </Action>
        <Action ID="LeafNodeIc120">
            <input_port default="ic120" name=" model_name"/>
            <inout_port name="record_name"/>
            <inout_port name="subtask_name"/>
        </Action>
        <Action ID="LeafNodeMst110cr">
            <input_port default="mst110cr" name=" model_name"/>
            <inout_port name="record_name"/>
            <inout_port name="subtask_name"/>
        </Action>
        <Action ID="LeafNodeMst2200">
            <input_port default="mst2200" name=" model_name"/>
            <inout_port name="record_name"/>
            <inout_port name="subtask_name"/>
        </Action>
        <Action ID="LeafNodeZx200">
            <input_port default="zx200" name=" model_name"/>
            <inout_port name="record_name"/>
            <inout_port name="subtask_name"/>
        </Action>
        <Action ID="MongoValueWriter">
            <input_port name="input_value"/>
            <input_port name="mongo_param_name"/>
            <input_port name="mongo_record_name"/>
        </Action>
        <Action ID="SetLocalBlackboard">
            <inout_port name="output_key"/>
            <input_port name="value"/>
        </Action>
        <Action ID="SetLocalBlackboardWithCounter">
            <inout_port name="counter_key"/>
            <input_port name="key"/>
        </Action>
    </TreeNodesModel>
    <!-- ////////// -->
</root>
